<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"

    android:id="@+id/Isnel"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

    <TextView
        android:id="@+id/Sedami"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Id :"
        android:layout_marginRight="15dp"
        android:textSize="20dp"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/txtid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="20dp"
        android:textColor="@color/black" />
</LinearLayout>

<LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/SedamiNom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nom :"
        android:layout_marginRight="15dp"
        android:textSize="20dp"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/txtnom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="20dp"
        android:textColor="@color/black" />
</LinearLayout>


<LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/SedamiPrenom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Prénom :"
        android:layout_marginRight="15dp"
        android:textSize="20dp"
        android:textColor="@color/black" />

    <TextView
        android:id="@+id/txtprenom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="20dp"
        android:textColor="@color/black" />
</LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/SedamiAdresse"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Adresse :"
            android:layout_marginRight="15dp"
            android:textSize="20dp"
            android:textColor="@color/black" />
    <TextView
        android:id="@+id/txtadresse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="20dp"
        android:textColor="@color/black" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
    <TextView
        android:id="@+id/SedamiMail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email :"
        android:layout_marginRight="15dp"
        android:textSize="20dp"
        android:textColor="@color/black" />
    <TextView
        android:id="@+id/txtemail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="20dp"
        android:textColor="@color/black" />
</LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/SedamiAge"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Age :"
            android:layout_marginRight="15dp"
            android:textSize="20dp"
            android:textColor="@color/black" />
    <TextView
        android:id="@+id/txtage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="20dp"
        android:textColor="@color/black" />
    </LinearLayout>


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/SedamiTel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Téléphone :"
            android:layout_marginRight="15dp"
            android:textSize="20dp"
            android:textColor="@color/black" />
    <TextView
        android:id="@+id/txttelephone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="20dp"
        android:textColor="@color/black" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/SedamiSpe"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Spécialité :"
            android:layout_marginRight="15dp"
            android:textSize="20dp"
            android:textColor="@color/black" />
    <TextView
        android:id="@+id/txtspecialite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="20dp"
        android:textColor="@color/black" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/SedamiEtu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Niveau d'Etude :"
            android:layout_marginRight="15dp"
            android:textSize="20dp"
            android:textColor="@color/black" />
    <TextView
        android:id="@+id/txtniveauEtude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="20dp"
        android:textColor="@color/black" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/SedamiExp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Expé Pro :"
            android:layout_marginRight="15dp"
            android:textSize="20dp"
            android:textColor="@color/black" />
    <TextView
        android:id="@+id/txtexperienceProfessionnelle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:textSize="20dp"
        android:textColor="@color/black" />
    </LinearLayout>


</LinearLayout>